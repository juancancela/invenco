{
  "name": "invencotest",
  "version": "1.0.1",
  "description": "invenco test",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "node node_modules/mocha/bin/mocha --recursive test",
    "coverage": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test/unit/domain/*.test.js node_modules/mocha/bin/_mocha test/unit/*.test.js --recursive -- -u exports -R spec && open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juancancela/invenco.git"
  },
  "keywords": [
    "invenco",
    "test",
    "javascript",
    "node",
    "nodejs"
  ],
  "author": "Juan Carlos Cancela <cancela.juancarlos@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juancancela/invenco/issues"
  },
  "bin":{
    "invencotest":"app.js"
  },
  "homepage": "https://github.com/juancancela/invenco#readme",
  "dependencies": {
    "istanbul": "0.4.5",
    "lodash": "^4.17.4",
    "mocha": "3.2.0",
    "restify": "^4.3.0"
  }
}
